{
  "name": "Webpack plugin manifest",
  "manifest_version": 2,
  "description": "description",
  "homepage_url": "https://github.com/daniel-hayes/generate-json-file-webpack-plugin",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "index.bundle.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "notifications"
  ]
}
